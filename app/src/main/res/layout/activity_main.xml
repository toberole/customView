        android:text="btn_test_ParallaxListview" />

    <Button
        android:id="@+id/btn_test_aes"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="btn_test_AES" />


    <Button
        android:id="@+id/btn_test_SpannableString"
